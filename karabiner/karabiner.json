        "rules": [
          {
            "description": "Remap Caps Lock to Ctrl when held, Esc when tapped",
            "manipulators": [
              {
                "from": {
                  "key_code": "caps_lock",
                  "modifiers": {
                    "optional": [
                      "any"
                    ]
                  }
                },
                "to": [
                  {
                    "key_code": "left_control",
                    "lazy": true
                  }
                ],
                "to_if_alone": [
                  {
                    "key_code": "escape"
                  }
                ],
                "type": "basic"
              }
            ]
          },
          {
            "description": "Remap Tab to Hyper key when held, Tab when tapped",
            "manipulators": [
              {
                "from": {
                  "key_code": "tab"
                },
                "to": [
                  {
                    "key_code": "left_shift",
                    "modifiers": [
                      "any"
                    ]
                  },
                  {
                    "set_variable": {
                      "name": "hyper",
                      "value": 1
                    }
                  }
                ],
                "to_after_key_up": [
                  {
                    "set_variable": {
                      "name": "hyper",
                      "value": 0
                    }
                  }
                ],
                "to_if_alone": [
                  {
                    "key_code": "tab"
                  }
                ],
                "type": "basic"
              }
            ]
          },
